 ! NOAA/PMEL TMAP
 ! FERRET v6.82  
 ! Darwin 9.8.0 - 08/06/12
 ! 29-May-15 18:12     

SET DATA "/Data/CMIP5/ESM2M/processed/rcp8.5/esm2m.rcp85.2081-2100.po2.nc", "/Data/Projects/CMIP5_p50/ESM2M_Blood/Meganyctiphanes_norvegica/p50av/esm2m.rcp85.2081-2100.p50av.Meganyctiphanes_norvegica.nc"
Let po2av = PO2[d=1, l=@ave]
Let diff = po2av - p50av
Let diffmin = diff[k=@min]
Let depthfind = diff - diffmin
shade depthfind
Let depthdiffmin = depthfind[z=@loc:0]
shade depthdiffmin
plot depthdiffmin[x=100w, y=0]
plot diffmin[x=100w, y=0]
plot diff[x=100w, y=0]
plot PO2[d=1, l=@ave, x==100w, y=0]
plot PO2[d=1, l=@ave, x=100w, y=0]
plot p50av[x=100w, y=0]
plot PO2[d=1, l=@ave, x=100w, y=0] p50av[x=100w, y=0]
plot PO2[d=1, l=@ave, x=100w, y=0], p50av[x=100w, y=0]
shade depthdiffmin
plot PO2[d=1, l=@ave, x=150w, y=60s], p50av[x=150w, y=60s]
shade depthdiffmin
go land
plot PO2[d=1, l=@ave, x=0, y=70n], p50av[x=0, y=70n]
quit
